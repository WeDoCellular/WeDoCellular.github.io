<script>
    $(function(){
		$('#subscriber-form').validate({
			messages: {
				email: {
					required: 'Please enter your email address',
					email: 'Please enter a valid email address'
				}
			},
			errorPlacement: function(error, el) {
                $('#error-span').html(error);
            },
			
			submitHandler: function(form) {
			    $("span.subscriber-result-div").hide();
				RC.Server.Account.subscribe(RC.Form.serialize($(form)),{
				    failure:function(context,msg,xml,jqXHR) {
						serversideFilure(context, msg, xml, jqXHR, $(form), 'Email Subscription Failed');
				    },
				    success:function(c,h,x){ 
				        bootbox.dialog({ 
        			        message:'<h4 class="cart-title" style="color:green;"><strong>Successful Subscription. </strong></h4><hr>You have subscribed to our newsletter with <strong>' + $('#newsletter-email').val() + '</strong>. Thank you!',
        			        buttons:{
        			            main:{
        			                label: "&nbsp;&nbsp;Close&nbsp;&nbsp;",
        			                className: "btn btn-default"
        			            }
        			        }
        			    });
            			    
				        $('#newsletter-email').val('');
				        
				    }
				});
			}
		});
	});
</script>
