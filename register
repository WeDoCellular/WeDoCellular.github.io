            <script>
                function getLayoutEncodedUrl(text) {
                    var currentUrl = $('#whatsapp-link-layout').attr('href');
                    $('#whatsapp-link-layout').attr('href', currentUrl + encodeURIComponent(text));
                }
            </script>
            
            
                
                    
                
            
