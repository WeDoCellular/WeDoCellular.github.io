   <script>
      function changePage(page) {
        if(page == 'signin') {
            $('#register-page').hide();
            $('#sign-in-page').show();
        }
        else
        {
            $('#register-page').show();
            $('#sign-in-page').hide();
        }
      }
      
	$(function(){
		$('#register-form').validate({
		    rules: {
                password1: { required: true, minlength: 6, equalTo: "#password-red" }
              },
			messages: {
				first_name: {
					required: 'Please enter your first name(s)',
					minlength: 'First name(s) must be 1-32 characters long',
				},
				surname: {
					required: 'Please enter your surname',
					minlength: 'Surname must be 1-32 characters long',
				},
				email: {
					required: 'Please enter your email address',
					minlength: 'Please enter a valid email address',
				},
				password: {
					required: 'Please enter your password',
					minlength: 'Password must be 6-20 characters long',
				},
				password1: {
					required: 'Please re-enter your password',
					minlength: 'Password must be 6-20 characters long',
					equalTo: 'Re-typed password is not the same, try again',
				},
				//terms_and_conditions: {
				//	required: 'Please accept our T&amp;C in order to continue',
				//},
			},
			submitHandler: function(form) {
				//if (!$('#terms_and_conditions').is(':checked')) {
				//	this.showErrors( { terms_and_conditions: 'You must accept our T&amp;C in order to continue!' } );
				//	return false;
				//}
				$(form).find('button[type="submit"] i').removeClass('fa-arrow-circle-right');
				$(form).find('button[type="submit"] i').addClass('fa-spinner fa-spin');
				$(form).find('button[type="submit"]').prop('disabled', true);
				$(form).find('#register-submit-failed').hide();
				
				RC.Server.Account.create(RC.Form.serialize($(form)),{
				    failure:function(context,msg,xml,jqXHR) {
    					serversideFilure(context, msg, xml, jqXHR, $(form), 'Registration Failed');
				    },
				    success:function(c,h,x){
            				    if(location.pathname=='/sign-in' || location.pathname=='/register')
    				                location.pathname='/account';
    				            else
    				                window.location.reload(); 
                			}
				});
			}
		});
		$('#first_name').focus();
	});
</script>

