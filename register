<script>
    $(function() {
        
            
                
                    
                        $( document ).ready(function() {
                            $('#global-subscription-popup-trigger').click();
                            setTimeout(function(){
                              $('#global_subscription_email')[0].focus();
                            }, 1000);
                        });
                    
                
            
         
        
        $('#global-subscriber-form').validate({
			messages: {
				email: {
					required: 'Please enter your email address',
					minlenght: 'Please enter your email address',
					email: 'Please enter a valid email address'
				}
			},
			errorPlacement: function(error, el) {
                $('#error-span').html(error);
            },
			submitHandler: function(form) {
			    RC.Server.Account.subscribe(RC.Form.serialize($(form)),{
    			    failure:function(context,msg,xml,jqXHR) {
    					bootbox.dialog({ 
                            message:'<h4 style="color:red;">Email Subscription Failed</h4><hr><p>' + msg + '</p>',
                            buttons:{
                                main:{
                                    label: "Close",
                                    className: "btn btn-mega "
                                    }
                                }
                        });
    			    },
    			    success:function(c,h,x){ 
    			        RC.Server.Session.Data.update({ subscription: 'subscribed' }, {
    			            success:function() {
    			                var code = $(x).find('data subscription_result').text();
                            	if (code == 'new') {
                            	    //Call to create coupon
                            	    
                                        $('.message-email').html($('#global_subscription_email').val());
                                        $('.initial').hide();
                                        $('.subscribed').show();
                                    
                            	}
                            	else if (code == 'already-exists') {
                            	    $('.message-email').html($('#global_subscription_email').val());
                                    $('.initial').hide();
                                    $('.already-existing').show();
                            	}
                            }
                        });
    			    }
    			});
			}
		});
		
		$('button[role="subscribe"]').on('click', function(){
            $('#global-subscriber-form').submit();
        });
        
        $('button[role="dismiss"]').on('click', function(){
            RC.Server.Session.Data.update({ subscription: 'dismissed' }, {success:function() { 
                    //alert('success dismiss'); 
                }
            });
        });
    });
</script>
