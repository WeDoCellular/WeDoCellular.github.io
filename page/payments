    function DeleteProductPopup(productId){
        RC.Server.Cart.remove({ itemId:productId },
            {success:function(){ 
                $('#cart-top-header').reload();
                $('#cart-updated-modal .modal-body').load('/cart/updated', function () {
                       
                    });
            }});
    }
